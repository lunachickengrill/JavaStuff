package com.addressmanagement.service.dto;

import java.io.Serializable;

public class AddressDetailDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private final String streetName;

	private final String streetNumber;

	private final String entrance;

	public AddressDetailDTO(String streetName, String streetNumber, String entrance) {
		this.streetName = streetName;
		this.streetNumber = streetNumber;
		this.entrance = entrance;
	}

	public String getStreetName() {
		return streetName;
	}

	public String getStreetNumber() {
		return streetNumber;
	}

	public String getEntrance() {
		return entrance;
	}

	@Override
	public String toString() {
		return String.format("AddressDetailDTO [streetName=%s, streetNumber=%s, entrance=%s]", streetName, streetNumber,
				entrance);
	}

}
